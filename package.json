    "@parcel/transformer-typescript-types": "^2.2.1",
    "@parcel/validator-typescript": "^2.2.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "@types/twemoji": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "c8": "^7.11.0",
    "eslint": "^8.8.0",
